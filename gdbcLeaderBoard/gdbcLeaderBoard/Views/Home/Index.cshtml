@model gdbcLeaderBoard.Models.HomeViewModels.ScoreOverviewViewModel

<div class="row">
    <h1>Team scores</h1>
    <ol>
        @for (int i = 0; i < Model.TeamScores.Count; i++)
        {
            <li><span>@Model.TeamScores[i].Venue - </span><span>@Model.TeamScores[i].Team </span> <span>@Model.TeamScores[i].Score</span></li>
        }
    </ol>
</div>

<div class="row">
    <h1>Venue scores</h1>
    <ol>
        @for (int i = 0; i < Model.VenueScores.Count; i++)
        {
            <li><span>@Model.VenueScores[i].Venue </span><span>@Model.VenueScores[i].Score</span></li>
        }
    </ol>
</div>
